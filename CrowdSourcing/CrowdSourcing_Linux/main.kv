<Menu>:
	name: "menu"
	FloatLayout:
        orientation: 'horizontal'
        canvas:
			Rectangle:
                source: "pic_z.png"
                size: self.size
                pos: self.pos 

	BoxLayout:
		orientation: "vertical"
        height: 10
		padding: [60, 20, 200, 300]
		spacing: 40

		Button:
			text: "К карте".encode('cp1251').decode('utf-8')
			color: 1,1,1,1
			background_color: 0.1, 0.5, 0.6, 1
			size_hint: None, None
			font_size: 25
			size: 290, 50
			opacity: 1
			on_release: app.root.current = "map"
		
		Button:
			text:"Заполнить анкету".encode('cp1251').decode('utf-8')
			color:1,1,1,1
			background_color: 0.1, 0.5, 0.6, 1
			size_hint: None, None
			font_size: 25
			size: 290, 50
			opacity: 1
			on_release: app.root.current = "form"
	AnchorLayout:
		anchor_x: 'center'
        anchor_y: 'top'
		Label:
			text:"CrowdSourcing\n"
			font_size: 40
			color: 0, 0, 0, 1
			size_hint:  None, None
		Label:
			text: "\n\nСледите за эпидемиологической\n      обстановкой в Ульяновске".encode('cp1251').decode('utf-8')
			font_size: 15
			color: 0, 0, 0, 1
			size_hint:  None, None
	

<Sites>:
	name:"sites"
	FloatLayout:
        orientation: 'horizontal'
        canvas:
            Rectangle:
                source: "pic_z.png"
                size: self.size
                pos: self.pos
    
